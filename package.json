    "jshint": "~2.6.3",
    "rcloader": "~0.1.4",
    "through2": "*"
  },
  "devDependencies": {
    "coveralls": "*",
    "event-stream": "*",
    "istanbul": "*",
    "istanbul-coveralls": "^1.0.1",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "should": "^8.3.2"
  },
  "engines": {
    "node": ">=0.9.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
